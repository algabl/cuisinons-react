{
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@cuisinons/prettier-config": "workspace:*",
    "@turbo/gen": "^2.5.4",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=9.6.0"
  },
  "name": "cuisinons",
  "packageManager": "pnpm@10.11.1",
  "prettier": "@cuisinons/prettier-config",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "auth:generate": "pnpm -F @cuisinons/auth generate",
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:push": "turbo -F @cuisinons/db push",
    "db:studio": "turbo -F @cuisinons/db studio",
    "dev": "turbo watch dev --continue",
    "dev:db": "docker compose up -d postgres",
    "dev:down": "docker compose down",
    "dev:full": "docker compose up -d postgres && turbo -F @cuisinons/db push && turbo watch dev --continue",
    "dev:next": "turbo watch dev -F @cuisinons/nextjs...",
    "format": "turbo run format --continue -- --cache --cache-location .cache/.prettiercache",
    "format:fix": "turbo run format --continue -- --write --cache --cache-location .cache/.prettiercache",
    "ios": "expo run:ios",
    "lint": "turbo run lint --continue -- --cache --cache-location .cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location .cache/.eslintcache",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint:ws",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add"
  },
  "version": "0.1.0",
  "pnpm": {
    "overrides": {
      "@types/minimatch": "npm:minimatch@latest"
    }
  }
}
